<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata" fqn="reportScheduler">
    <apiVersion>48.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>NegOptim Report Scheduler</masterLabel>
    <targets>
        <target>lightning__AppPage</target>
        <target>lightning__Tab</target>
        <target>lightning__HomePage</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__HomePage,lightning__AppPage">
            <property name="folderId" type="String" label="Folder Name:" datasource="apex://ReportsGridDataSource" description="The folder Name of the reports"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>